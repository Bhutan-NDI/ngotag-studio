{
    "family": "	QA-FRONTEND_UI_TASKDEF",
    "containerDefinitions": [
        {
            "name": "frontend_ui",
            "image": "%REPOSITORY_URI%:FRONTEND_V_%BUILD_NUMBER%",
            "cpu": 1024,
            "memory": 2048,
            "portMappings": [
                {
                    "containerPort": 8085,
                    "hostPort": 8085,
                    "protocol": "tcp"
                }
            ],
            "essential": true,
            "environment": [],
            "environmentFiles": [],
            "mountPoints": [],
            "volumesFrom": [],
            "ulimits": [],
            "logConfiguration": {
                "logDriver": "awslogs",
                "options": {
                    "awslogs-create-group": "true",
                    "awslogs-group": "/ecs/QA-FRONTEND_UI_TASKDEF",
                    "awslogs-region": "ap-southeast-1",
                    "awslogs-stream-prefix": "ecs"
                },
                "secretOptions": []
            }
        }
        
    ],
   
    "executionRoleArn": "arn:aws:iam::id:role/ecsTaskExecutionRole",
    "networkMode": "awsvpc",
    "placementConstraints": [],
    "requiresCompatibilities": [
        "FARGATE"
    ],
    "cpu": "1024",
    "memory": "2048"
}
